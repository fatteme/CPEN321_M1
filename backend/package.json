{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "format": "prettier --check \"src/**/*.{ts,js,json}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,js,json}\"",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "code-quality": "npm run lint && npm run format"
  },
  "author": "",
  "license": "ISC",
  "description": "TypeScript backend project",
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^4.18.0",
    "google-auth-library": "^10.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
